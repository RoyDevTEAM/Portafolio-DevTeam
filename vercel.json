{
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "script-src 'self' https://pagead2.googlesyndication.com https://www.googletagservices.com; object-src 'self'"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/sitemap.xml",
        "destination": "/sitemap.xml"
      }
    ]
  }
  